{
	"compilerOptions": {
		/* Basic Options */
		"target": "ES2022",
		"module": "CommonJS",
		"declaration": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"skipLibCheck": true,
		"importHelpers": true,
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@types/*" : ["./src/types/*"],
			"@handlers/*" : ["src/handlers/*"],
			"@utils/*" : ["./src/utils/*"],
		},
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"types": [ "node", "ws" ]
	},
	"exclude": [
	  "node_modules",
	  "dist",
	  "testBot",
	]
  }